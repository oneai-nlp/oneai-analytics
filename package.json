{
  "version": "0.2.305",
  "license": "MIT",
  "main": "dist/index",
  "module": "dist/oneai-analytics.esm",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/oneai-analytics.esm.js"
    }
  },
  "description": "OneAI analytics package",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/power-of-language/oneai-analytics.git"
  },
  "keywords": [
    "react",
    "typescript",
    "oneAI"
  ],
  "files": [
    "src",
    "dist",
    "LICENSE",
    "README.md"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "start": "dts watch",
    "build": "dts build",
    "test": "dts test --passWithNoTests --env=jsdom",
    "lint": "dts lint --fix",
    "prepare": "dts build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "sb": "storybook dev -p 6006 --no-open",
    "dev": "storybook dev -p 6006 --no-open",
    "build-storybook": "NODE_OPTIONS=--openssl-legacy-provider storybook build -o docs-build"
  },
  "peerDependencies": {
    "react": ">=18.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "oneai-analytics",
  "author": "Ron Helfer",
  "size-limit": [
    {
      "path": "dist/oneai-analytics.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/oneai-analytics.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@size-limit/preset-small-lib": "^8.2.4",
    "@storybook/addon-essentials": "^7.0.11",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^7.0.11",
    "@storybook/addon-mdx-gfm": "^7.0.11",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addons": "^7.0.11",
    "@storybook/cli": "^7.0.11",
    "@storybook/react": "^7.0.11",
    "@storybook/react-webpack5": "^7.0.11",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/chroma-js": "^2.4.0",
    "@types/d3": "^7.4.0",
    "@types/jest": "^29.5.1",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@types/react-resize-detector": "^6.1.0",
    "autoprefixer": "^10.4.14",
    "babel-jest": "^29.5.0",
    "babel-loader": "^9.1.2",
    "dts-cli": "^2.0.3",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "postcss": "^8.4.23",
    "postcss-prefix-selector": "^1.16.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "rollup-plugin-postcss": "^4.0.2",
    "size-limit": "^8.2.4",
    "storybook": "^7.0.11",
    "tailwind-scrollbar": "^3.0.2",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.0",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.14",
    "@heroicons/react": "^2.0.18",
    "chroma-js": "^2.4.2",
    "core-js": "^3.30.2",
    "d3": "7.8.4",
    "date-fns": "^2.30.0",
    "lottie-web": "^5.12.2",
    "moment": "^2.29.4",
    "react-app-polyfill": "^3.0.0",
    "react-focus-on": "^3.8.1",
    "react-papaparse": "^4.1.0",
    "react-resize-detector": "^9.1.0",
    "react-tooltip": "^4.5.1",
    "resize-observer-polyfill": "^1.5.1"
  }
}
