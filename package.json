{
  "version": "0.2.24",
  "license": "MIT",
  "main": "dist/index",
  "module": "dist/oneai-analytics.esm",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/oneai-analytics.esm.js"
    }
  },
  "description": "OneAI analytics package",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/power-of-language/oneai-analytics.git"
  },
  "keywords": [
    "react",
    "typescript",
    "oneAI"
  ],
  "files": [
    "src",
    "dist",
    "LICENSE",
    "README.md"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "start": "dts watch",
    "build": "dts build",
    "test": "dts test --passWithNoTests --env=jsdom",
    "lint": "dts lint --fix",
    "prepare": "dts build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "sb": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o docs-build"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "oneai-analytics",
  "author": "Ron Helfer",
  "size-limit": [
    {
      "path": "dist/oneai-analytics.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/oneai-analytics.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@size-limit/preset-small-lib": "^8.1.0",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addons": "^6.5.13",
    "@storybook/react": "^6.5.13",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/chroma-js": "^2.1.4",
    "@types/d3": "^7.4.0",
    "@types/jest": "^29.2.2",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "autoprefixer": "^10.4.13",
    "babel-jest": "^29.3.1",
    "babel-loader": "^9.1.0",
    "dts-cli": "^1.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "postcss": "^8.4.18",
    "postcss-prefix-selector": "^1.16.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "rollup-plugin-postcss": "^4.0.2",
    "size-limit": "^8.1.0",
    "tailwind-scrollbar": "^2.0.1",
    "tailwindcss": "^3.2.3",
    "ts-jest": "^29.0.3",
    "tslib": "^2.4.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.4",
    "@heroicons/react": "^2.0.13",
    "chroma-js": "^2.4.2",
    "core-js": "^3.26.0",
    "d3": "7.6.1",
    "date-fns": "^2.29.3",
    "react-app-polyfill": "^3.0.0",
    "react-resize-detector": "^7.1.2",
    "react-tooltip": "^4.5.0",
    "resize-observer-polyfill": "^1.5.1"
  }
}
