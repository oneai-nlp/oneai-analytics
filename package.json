{
  "version": "0.2.28",
  "license": "MIT",
  "main": "dist/index",
  "module": "dist/oneai-analytics.esm",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/oneai-analytics.esm.js"
    }
  },
  "description": "OneAI analytics package",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/power-of-language/oneai-analytics.git"
  },
  "keywords": [
    "react",
    "typescript",
    "oneAI"
  ],
  "files": [
    "src",
    "dist",
    "LICENSE",
    "README.md"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "start": "dts watch",
    "build": "dts build",
    "test": "dts test --passWithNoTests --env=jsdom",
    "lint": "dts lint --fix",
    "prepare": "dts build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "sb": "start-storybook -p 6006",
    "dev": "start-storybook -p 6006",
    "build-storybook": "NODE_OPTIONS=--openssl-legacy-provider build-storybook -o docs-build"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "oneai-analytics",
  "author": "Ron Helfer",
  "size-limit": [
    {
      "path": "dist/oneai-analytics.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/oneai-analytics.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@size-limit/preset-small-lib": "^8.2.4",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addons": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/chroma-js": "^2.4.0",
    "@types/d3": "^7.4.0",
    "@types/jest": "^29.4.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-resize-detector": "^6.1.0",
    "autoprefixer": "^10.4.13",
    "babel-jest": "^29.4.3",
    "babel-loader": "^9.1.2",
    "dts-cli": "^1.6.3",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "postcss": "^8.4.21",
    "postcss-prefix-selector": "^1.16.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "rollup-plugin-postcss": "^4.0.2",
    "size-limit": "^8.2.4",
    "tailwind-scrollbar": "^2.1.0",
    "tailwindcss": "^3.2.7",
    "ts-jest": "^29.0.5",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.11",
    "@heroicons/react": "^2.0.16",
    "chroma-js": "^2.4.2",
    "core-js": "^3.28.0",
    "d3": "7.8.2",
    "date-fns": "^2.29.3",
    "moment": "^2.29.4",
    "react-app-polyfill": "^3.0.0",
    "react-papaparse": "^4.1.0",
    "react-resize-detector": "^8.0.4",
    "react-tooltip": "^4.5.1",
    "resize-observer-polyfill": "^1.5.1"
  }
}